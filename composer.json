{
    "name": "smartdata/smartdata",
    "repositories": [
        {"type": "vcs", "url": "git@github.com:gabrielbull/geotools.git"}
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "2.*",
        "psr/log": "~1",
        "maxmind-db/reader": "0.3.*",
        "league/geotools": "dev-feature/polygon"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-4": {
            "SmartData\\SmartData\\": "src/SmartData"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartData\\SmartData\\Tests\\": ["tests/SmartData/Tests", "tests/SmartData/Tests/_includes"]
        }
    },
    "bin": [
        "bin/smartdata"
    ]
}