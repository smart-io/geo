{
  "name": "smart/geo",
  "type": "library",
  "description": "Smart Geo is databases from Open Data providers compiled into easy to use PHP objects.",
  "keywords": ["Country", "Region", "Data", "Database", "GeoIp", "Geolocation", "Province", "State"],
  "homepage": "https://github.com/smart-io/geo",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Bull",
      "email": "me@gabrielbull.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "league/geotools": "~0.6",
    "maxmind-db/reader": "~1.0",
    "psr/log": "~1",
    "symfony/console": "~2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "~5"
  },
  "autoload": {
    "psr-4": {
      "Smart\\Geo\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smart\\Geo\\Tests\\": [
        "tests/Geo/Tests",
        "tests/Geo/Tests/_includes"
      ]
    }
  },
  "bin": [
    "bin/geo",
    "bin/geo.php"
  ]
}
